    "jquery-validation": "1.15.1",
    "reset.css": "2.0.2",
    "respond.js": "1.4.2"
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "babel-node  ./node_modules/es-webpack-engine/dist/chokidar-watch.js && cross-env NODE_ENV=development nodemon ./app/Resources/webpack/webpack.dev.js --exec babel-node --max_old_space_size=4096",
    "compile": "cross-env NODE_ENV=production babel-node --max_old_space_size=4096 ./node_modules/.bin/webpack --progress --config ./app/Resources/webpack/webpack.js",
    "compile:debug": "npm run compile -- --debugMode=true",
    "lint": "eslint app/Resources/static-src/**/*.js",
    "clean": "npm prune"
  }
