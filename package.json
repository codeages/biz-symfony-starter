    "webpack": "1.14.0"
  },
  "dependencies": {
    "babel-polyfill": "6.5.0",
    "admin-lte": "2.3.6",
    "bootstrap": "3.3.7",
    "bootstrap-notify": "3.1.3",
    "es-ckeditor": "latest",
    "font-awesome": "4.6.3",
    "html5shiv": "3.7.3",
