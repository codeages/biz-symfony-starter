    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.11.1",
    "babel-runtime": "6.20.0",
    "cross-env": "3.1.4",
    "es-webpack-engine": "~2.5.2",
    "eslint": "3.16.0",
    "eslint-config-airbnb": "14.1.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
